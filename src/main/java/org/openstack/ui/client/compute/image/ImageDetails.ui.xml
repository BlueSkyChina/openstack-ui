<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.details {
			width: 100%;
			table-layout: fixed;
		}
		.title {
			font-weight: bold;
		}
	</ui:style>
	<g:HorizontalPanel styleName="{style.details}">
		<g:HTMLPanel>
			<table class="table table-stripped {style.details}">
				<tbody>
					<tr>
						<td class="{style.title}">ID</td>
						<td>
							<g:Label ui:field="id" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">Checksum</td>
						<td>
							<g:Label ui:field="checksum" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">Container Format</td>
						<td>
							<g:Label ui:field="containerFormat" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">Created At</td>
						<td>
							<g:Label ui:field="createdAt" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">Updated At</td>
						<td>
							<g:Label ui:field="updatedAt" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">Deleted</td>
						<td>
							<g:Label ui:field="deleted" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">Deleted At</td>
						<td>
							<g:Label ui:field="deletedAt" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">Disk Format</td>
						<td>
							<g:Label ui:field="diskFormat" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">Protected</td>
						<td>
							<g:Label ui:field="isProtected" />
						</td>
					</tr>
				</tbody>
			</table>
		</g:HTMLPanel>
		<g:HTMLPanel>
			<table class="table table-stripped {style.details}">
				<tbody>
					<tr>
						<td class="{style.title}">Public</td>
						<td>
							<g:Label ui:field="isPublic" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">minDisk</td>
						<td>
							<g:Label ui:field="minDisk" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">minRam</td>
						<td>
							<g:Label ui:field="minRam" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">name</td>
						<td>
							<g:Label ui:field="name" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">owner</td>
						<td>
							<g:Label ui:field="ownerx" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">size</td>
						<td>
							<g:Label ui:field="size" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">status</td>
						<td>
							<g:Label ui:field="status" />
						</td>
					</tr>
					<tr>
						<td class="{style.title}">uri</td>
						<td>
							<g:Label ui:field="uri" />
						</td>
					</tr>
				</tbody>
			</table>
		</g:HTMLPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 