<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.details {
			width: 100%;
			table-layout: fixed;
		}
	</ui:style>
	<g:HorizontalPanel styleName="{style.details}">
		<g:HTMLPanel>
			<table class="{style.details}">
				<tbody>
					<tr>
						<td>ID</td>
						<td>
							<g:Label ui:field="id" />
						</td>
					</tr>
					<tr>
						<td>Name</td>
						<td>
							<g:Label ui:field="name" />
						</td>
					</tr>
					<tr>
						<td>Description</td>
						<td>
							<g:Label ui:field="description" />
						</td>
					</tr>
					<tr>
						<td>Enabled</td>
						<td>
							<g:Label ui:field="enabled" />
						</td>
					</tr>
				</tbody>
			</table>
		</g:HTMLPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 