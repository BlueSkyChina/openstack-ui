<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HeaderPanel width="600px" height="450px">
		<g:HTMLPanel>
			<div class="modal-header" style="margin-bottom: 15px">
				<g:Anchor ui:field="close" styleName="close">Ã—</g:Anchor>
				<h3>Create Key Pair</h3>
			</div>
		</g:HTMLPanel>
		<g:ResizeLayoutPanel height="100%">
			<g:HorizontalPanel>
				<g:FormPanel ui:field="formPanel" action="/ui/keypair-download"
					method="POST">
					<g:FlowPanel>
						<g:TextBox ui:field="keyPairName" name="keyPairName" />
						<g:Hidden ui:field="computeURL" name="computeURL" />
						<g:Hidden ui:field="token" name="token" />
					</g:FlowPanel>
				</g:FormPanel>
				<g:Anchor ui:field="createKeyPair">Create &amp; Download Key Pair</g:Anchor>
			</g:HorizontalPanel>
		</g:ResizeLayoutPanel>
		<g:HTMLPanel ui:field="footer" styleName="modal-footer">
			<g:Button ui:field="save" styleName="btn">Save</g:Button>
			<g:Button ui:field="cancel" styleName="btn">Cancel</g:Button>
		</g:HTMLPanel>
	</g:HeaderPanel>
</ui:UiBinder> 