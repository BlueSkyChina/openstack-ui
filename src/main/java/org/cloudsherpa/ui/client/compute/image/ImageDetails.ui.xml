<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.details {
			width: 100%;
			table-layout: fixed;
		}
	</ui:style>
	<g:HorizontalPanel styleName="{style.details}">
		<g:HTMLPanel>
			<table class="{style.details}">
				<tbody>
					<tr>
						<td>ID</td>
						<td>
							<g:Label ui:field="id" />
						</td>
					</tr>
					<tr>
						<td>Checksum</td>
						<td>
							<g:Label ui:field="checksum" />
						</td>
					</tr>
					<tr>
						<td>Container Format</td>
						<td>
							<g:Label ui:field="containerFormat" />
						</td>
					</tr>
					<tr>
						<td>Created At</td>
						<td>
							<g:Label ui:field="createdAt" />
						</td>
					</tr>
					<tr>
						<td>Updated At</td>
						<td>
							<g:Label ui:field="updatedAt" />
						</td>
					</tr>
					<tr>
						<td>Deleted</td>
						<td>
							<g:Label ui:field="deleted" />
						</td>
					</tr>
					<tr>
						<td>Deleted At</td>
						<td>
							<g:Label ui:field="deletedAt" />
						</td>
					</tr>
					<tr>
						<td>Disk Format</td>
						<td>
							<g:Label ui:field="diskFormat" />
						</td>
					</tr>
					<tr>
						<td>Protected</td>
						<td>
							<g:Label ui:field="isProtected" />
						</td>
					</tr>
				</tbody>
			</table>
		</g:HTMLPanel>
		<g:HTMLPanel>
			<table class="{style.details}">
				<tbody>
					<tr>
						<td>Public</td>
						<td>
							<g:Label ui:field="isPublic" />
						</td>
					</tr>
					<tr>
						<td>minDisk</td>
						<td>
							<g:Label ui:field="minDisk" />
						</td>
					</tr>
					<tr>
						<td>minRam</td>
						<td>
							<g:Label ui:field="minRam" />
						</td>
					</tr>
					<tr>
						<td>name</td>
						<td>
							<g:Label ui:field="name" />
						</td>
					</tr>
					<tr>
						<td>owner</td>
						<td>
							<g:Label ui:field="ownerx" />
						</td>
					</tr>
					<tr>
						<td>size</td>
						<td>
							<g:Label ui:field="size" />
						</td>
					</tr>
					<tr>
						<td>status</td>
						<td>
							<g:Label ui:field="status" />
						</td>
					</tr>
					<tr>
						<td>uri</td>
						<td>
							<g:Label ui:field="uri" />
						</td>
					</tr>
				</tbody>
			</table>
		</g:HTMLPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 